{
  "name": "vue3-element-admin",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "sync:viewids": "npx cross-env DOTENV_CONFIG_PATH=.env.dev_test tsx scripts/sync-viewids.ts",
    "sync:role-permissions": "npx cross-env DOTENV_CONFIG_PATH=.env.dev_test tsx scripts/sync-role-permissions.ts",
    "sync:all": "npm run sync:viewids && npm run sync:role-permissions",
    "dev:test": "npm run sync:all && vite --mode dev_test",
    "dev:prod": "npm run sync:all && vite --mode dev_prod",
    "build:test": "vite build --mode build_test",
    "build:prod": "vite build --mode build_prod",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@tanstack/vue-query": "^5.82.0",
    "axios": "^1.7.2",
    "clipboard": "^2.0.11",
    "driver.js": "^1.3.1",
    "dropzone": "^5.9.3",
    "echarts": "^5.5.0",
    "element-plus": "^2.7.6",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.6.2",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.2",
    "pinia": "^2.1.7",
    "sortablejs": "^1.15.2",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue-toastification": "2.0.0-rc.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.23.0",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.2.0",
    "rollup-plugin-svg-sprites": "^1.2.5",
    "sass": "^1.77.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.1",
    "vite": "^5.3.1",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-mock": "^2.9.8",
    "vue-tsc": "^2.0.21",
    "dotenv": "^16.6.0"
  }
}
